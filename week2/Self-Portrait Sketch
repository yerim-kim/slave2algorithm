//Sketch 31/7/2020

void setup(){
size(500,500);
background(64,120,240);
}

void draw(){
fill(241,218,203);
ellipse(250,100,400,500);
ellipse(250,400,100,200);
fill(0);
ellipse(300, 20, 300, 150);
ellipse(100,20,150,150);
fill(0);
rect(100,100,80,50);
rect(300,100,80,50);
fill(75,156,211);
ellipse(250,600,400,500);
fill(0);
stroke(0);
rect (mouseX,mouseY,50,50);
stroke(0,255,0);
fill(255,0,0);
//rect(20,20,40,60);
fill(mouseX/2,mouseY/2,(mouseX+mouseY)/4);
noStroke();
}
